[{"name":"imagecontent-component","contentData":{"backgroundImage":"https://stagestphilemons.blob.core.windows.net/image-container/Screen%20Shot%202019-01-31%20at%2012.42.00.png","backgroundImageWidth":81,"backgroundImageHeight":41,"mobileBackgroundImage":"https://stagestphilemons.blob.core.windows.net/image-container/Screen%20Shot%202019-01-31%20at%2012.42.05.png","mobileBackgroundImageWidth":30,"mobileBackgroundImageHeight":30},"hide":false,"showInMenu":false,"menuText":null,"menuIcon":null},{"name":"menucontent-component","contentData":{"items":null},"hide":false,"showInMenu":false,"menuText":null,"menuIcon":null},{"name":"imagecontent-component","contentData":{"backgroundImage":"https://stagestphilemons.blob.core.windows.net/image-container/Screen%20Shot%202019-01-31%20at%2012.42.42.png","backgroundImageWidth":89,"backgroundImageHeight":43,"mobileBackgroundImage":"https://stagestphilemons.blob.core.windows.net/image-container/Screen%20Shot%202019-01-31%20at%2012.42.49.png","mobileBackgroundImageWidth":82,"mobileBackgroundImageHeight":55},"hide":false,"showInMenu":true,"menuText":null,"menuIcon":"https://stagestphilemons.blob.core.windows.net/image-container/Screen%20Shot%202019-01-31%20at%2012.42.49.png"},{"name":"textcontent-component","contentData":{"text":"<p class=\"MsoNormal\">&nbsp;</p>\n<p class=\"MsoNormal\">If you're looking after little children during the day time, you're really welcome at Mini Phillies, our 'stay and play' group. We have toys, singing, snacks and craft for the little ones, and sofas, hot <span style=\"color: #ffffff;\">drinks,</span> and adult compa<span style=\"color: #ffffff;\">ny for you.</span></p>\n<p class=\"MsoNormal\">&nbsp;</p>\n<p class=\"MsoNormal\"><span style=\"color: #ffffff;\"><strong>Who for?</strong> 0-4 year olds</span></p>\n<p class=\"MsoNormal\"><span style=\"color: #ffffff;\"><strong>When?</strong> 9.30-11.15am every Wednesday during term time</span></p>\n<p class=\"MsoNormal\"><span style=\"color: #ffffff;\"><strong>How much?</strong> It's free!</span></p>\n<p class=\"MsoNormal\"><span style=\"color: #ffffff;\"><strong>More info?</strong> Email sarah@stphilemons.co.uk, check out our&nbsp;<a style=\"color: #ffffff;\" href=\"https://www.facebook.com/MiniPhillies/\"><span style=\"text-decoration: none;\"><span style=\"text-decoration: underline;\">Facebook page</span></span></a>, or just</span> drop in!</p>\n<p class=\"MsoNormal\">&nbsp;</p>","backgroundColor":"white"},"hide":false,"showInMenu":false,"menuText":null,"menuIcon":null},{"name":"imagecontent-component","contentData":{"backgroundImage":"https://stagestphilemons.blob.core.windows.net/image-container/Screen%20Shot%202019-01-31%20at%2012.45.28.png","backgroundImageWidth":109,"backgroundImageHeight":44,"mobileBackgroundImage":"https://stagestphilemons.blob.core.windows.net/image-container/Screen%20Shot%202019-01-31%20at%2012.45.32.png","mobileBackgroundImageWidth":47,"mobileBackgroundImageHeight":41},"hide":false,"showInMenu":true,"menuText":null,"menuIcon":"https://stagestphilemons.blob.core.windows.net/image-container/Screen%20Shot%202019-01-31%20at%2012.45.32.png"},{"name":"textcontent-component","contentData":{"text":"<p class=\"MsoNormal\">Every Sunday morning, we'll take your 0-4s off your hands (if you want us to) for the duration of the service.</p>\n<p class=\"MsoNormal\">We're a team of trained, DBS-vetted adults who love children and don't mind a bit of noise.&nbsp;</p>","backgroundColor":"white"},"hide":false,"showInMenu":false,"menuText":null,"menuIcon":null},{"name":"imagecontent-component","contentData":{"backgroundImage":"https://stagestphilemons.blob.core.windows.net/image-container/Screen%20Shot%202019-01-31%20at%2012.47.35.png","backgroundImageWidth":115,"backgroundImageHeight":62,"mobileBackgroundImage":"https://stagestphilemons.blob.core.windows.net/image-container/Screen%20Shot%202019-01-31%20at%2012.47.40.png","mobileBackgroundImageWidth":113,"mobileBackgroundImageHeight":91},"hide":false,"showInMenu":true,"menuText":null,"menuIcon":"https://stagestphilemons.blob.core.windows.net/image-container/Screen%20Shot%202019-01-31%20at%2012.47.40.png"},{"name":"textcontent-component","contentData":{"text":"<p class=\"MsoNormal\">&nbsp;</p>\n<p class=\"MsoNormal\">A club for children from age 3 to school year 2.</p>\n<p class=\"MsoNormal\">Parents can drop off children or stay with them.</p>\n<p class=\"MsoNormal\">Every Friday in term time. 4-5pm.</p>\n<p class=\"MsoNormal\">Free</p>\n<p class=\"MsoNormal\">For more information, contact Cat (07535 668093).&nbsp;</p>","backgroundColor":"white"},"hide":false,"showInMenu":false,"menuText":null,"menuIcon":null},{"name":"imagecontent-component","contentData":{"backgroundImage":"https://stagestphilemons.blob.core.windows.net/image-container/Screen%20Shot%202019-01-31%20at%2012.50.56.png","backgroundImageWidth":107,"backgroundImageHeight":49,"mobileBackgroundImage":"https://stagestphilemons.blob.core.windows.net/image-container/Screen%20Shot%202019-01-31%20at%2012.51.00.png","mobileBackgroundImageWidth":82,"mobileBackgroundImageHeight":59},"hide":false,"showInMenu":true,"menuText":null,"menuIcon":"https://stagestphilemons.blob.core.windows.net/image-container/Screen%20Shot%202019-01-31%20at%2012.51.00.png"},{"name":"textcontent-component","contentData":{"text":"<p class=\"MsoNormal\">&nbsp;</p>\n<p class=\"MsoNormal\">While Mums and Dads are in the service (both 11am and 5pm), our 4-11 year olds have a group. We sing, make crafts, play football/games and listen to a story from the Bible so we can get to know God.</p>\n<p class=\"MsoNormal\">We aim to make Sunday the best day of the week for the children so we have a lot of fun. And someone always wins the trophy at the end!</p>","backgroundColor":"white"},"hide":false,"showInMenu":false,"menuText":null,"menuIcon":null},{"name":"imagecontent-component","contentData":{"backgroundImage":"https://stagestphilemons.blob.core.windows.net/image-container/Screen%20Shot%202019-01-31%20at%2012.49.17.png","backgroundImageWidth":111,"backgroundImageHeight":44,"mobileBackgroundImage":"https://stagestphilemons.blob.core.windows.net/image-container/Screen%20Shot%202019-01-31%20at%2012.49.22.png","mobileBackgroundImageWidth":77,"mobileBackgroundImageHeight":57},"hide":false,"showInMenu":false,"menuText":null,"menuIcon":"https://stagestphilemons.blob.core.windows.net/image-container/Screen%20Shot%202019-01-31%20at%2012.49.22.png"},{"name":"textcontent-component","contentData":{"text":"<p class=\"MsoNormal\">&nbsp;</p>\n<p class=\"MsoNormal\">Fun, football, games, Wii, crafts, cooking, dodgeball, ping pong, basketball and 'Bible time' to hear about Jesus.</p>\n<p class=\"MsoNormal\">6-7.30pm. Every Friday in term time.</p>\n<p class=\"MsoNormal\">For school years 3-6.</p>\n<p class=\"MsoNormal\">Only &pound;1.</p>\n<p class=\"MsoNormal\">For more information, contact Alice (07815536648).&nbsp;</p>","backgroundColor":"white"},"hide":false,"showInMenu":false,"menuText":null,"menuIcon":null}]